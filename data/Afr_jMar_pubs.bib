@article{Lluch-Belda1992a,
abstract = {A model previously proposed to explain the expansion of the population of the Californian sardine may also explain fluctuations in the South American sardine. In this paper, the frequency of spawning and landings, as related to sea surface temperature, are analysed for both. This information, together with evidence previously presented, indicates a similar pattern of population expansion and contraction in both areas. Off the Pacific coasts of North and South America, sustained warm periods are associated with greater abundance and extended spawning of sardine.},
author = {Lluch-Belda, D and Lluch-Cota, D B and Hernandez-Vazquez, S and Salinas-Zavala, C A C N - 138},
issn = {0257-7615},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{Sardine Population Expansion in Eastern Boundary Systems of the Pacific-Ocean as Related to Sea-Surface Temperature}},
year = {1992}
}
@article{Waluda1998c,
abstract = {A Geographic Information System (GIS) was used to test hypotheses regarding the spatial distribution of the squid Loligo forbesi and Loligo vulgaris vulgaris in the northern North-East Atlantic during the years 1989-1994. Loligo spp. were present throughout coastal waters of the United Kingdom, but distribution was patchy and highly variable over space and time. The relationship between squid distribution and sea temperature and salinity in the North Sea was examined by overlaying maps of squid abundance (landings per unit effort lpue) and oceanographic variables, and using correlation and multiple regression analysis. Bottom temperature was most frequently correlated with lpue, with correlations between squid and oceanographic parameters occurring most often in January and February of a given year. Monthly changes in distribution patterns are consistent with squid undertaking seasonal migrations around the coast of the U.K. It is at present unknown whether temperature and salinity influence squid distribution directly (e.g. reflecting physiological tolerance limits), through effects on recruitment, growth or mortality, or indirectly as a result of passive movement of squid into the North Sea during inflow of warmer Atlantic water},
author = {Waluda, C M and Pierce, G J},
journal = {South African Journal of Marine Science - Suid-Afrikaanse Tydskrif Vir Seewetenskap},
keywords = {ABUNDANCE,ATLANTIC,GROWTH,SQUID,TEMPERATURE,distribution,recruitment},
title = {{Temporal and spatial patterns in the distribution of squid Loligo spp. in United Kingdom waters}},
year = {1998}
}
@article{Cockcroft1991b,
abstract = {Sea Fisheries Research Inst Dept Environment Affairs The offshore distribution and abundance of bottlenose dolphins tursiops truncatus along the Natal south coast was assessed aerially in may of 1989 and 1990. Unlike off the Natal north coast and the eastern cape, a substantial proportion of sightings was made in water deeper than 30 m. This indicates that distribution on the south coast may be different from that in other areas, possibly because of the presence there of offshore reefs, the continued degradation of the Natal inshore environment and the 1987 floods. There were probably fewer than 350 bottlenose dolphins off the south coast, an apparent decline since the last survey of the area in 1985. Incidental captures of bottlenose dolphins on the south coast average 24 animals per year, more than 7 per cent of the estimated maximum number. The consequences of this and also of the continued degradation of Natal's coastal zone are examined},
author = {Cockcroft, V.G. and Ross, G.J.B. and Peddemors, V.M.},
journal = {South African Journal Of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{Distribution and status of bottle-nosed-dolphin tursiops truncatus on the south coast of Natal, South Africa}},
year = {1991}
}
@article{Freer1993b,
abstract = {St Joseph Callorhinchus capensis are caught by a directed gillnet$\backslash$nfishery off the coast of South Africa's Western Cape and also incidentally$\backslash$nby trawl in shallow water on both South and West coasts. The fishery$\backslash$nonly became established in the early 1980s and landings have subsequently$\backslash$nstabilized at approximately 800 tons per year, almost one-third of$\backslash$nwhich is a by-catch of trawling operations. The gillnet fishery is$\backslash$ncentred in St Helena Bay and takes very little by-catch. Both trawl$\backslash$nand gillnet operations take place in nursery areas of C. capensis$\backslash$nand some species of true shark. C. capensis occurs widely off the$\backslash$nsouthern African coast, from 1 to 366 m deep, but large numbers are$\backslash$nonly caught inshore ({\textless}50 m), the only area where immature fish are$\backslash$nfound. The fork length at 50{\%} sexual maturity is 435 mm in males$\backslash$n(3,3 years) and 496 mm in females (4,2 years). Mature fish have an$\backslash$nannual onshore/offshore migration, most mature fish aggregating inshore$\backslash$nin summer to breed and lay eggs. They then disperse to deep water$\backslash$nwhere unlaid eggs are resorbed and energy reserves accumulated. The$\backslash$nlargest number of viable eggs recorded was 22. C. capensis primarily$\backslash$nlive over soft substrata and their diet consists principally of invertebrates$\backslash$nassociated with such substrata. The relatively fast growth, early$\backslash$nsexual maturity and reasonably high fecundity of the species make$\backslash$nit a safer fisheries target than most other chondrichthyans.},
author = {Freer, D W L and Griffiths, C L},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{The fishery for, and general biology of, the St-Joseph Callorhinchus capensis (Dumeril) off the South-Western Cape, South-Africa}},
year = {1993}
}
@article{Cockcroft1990g,
abstract = {The seasonal abundance and distribution of common dolphins on the south-east coast of southern Africa, between Port Elizabeth and Richards Bay, was studied from aerial sightings made between March 1988 and August 1989.  Common dolphins appear to be present between Port Elizabeth and East London throughout the year, but their density there is low.  Farther north, along the coasts of Transkei and Natal, common dolphins are found only during winter, when they apparently migrate there at the same time as pilchard in the annual Natal "sardine run".  Densities off Transkei and Natal are greater than they are off the Eastern Cape, but this may only be an artifact of the narrowness of the continental shelf in the north.  Common dolphins have probably adapted to the plentiful food resources provided by the sardine run, and females may use the migration to wean their young and replenish energy reserves before their next pregnancy.},
author = {Cockcroft, V G and Peddemors, V M},
isbn = {0257-7615},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{Seasonal Distribution and Density of Common Dolphins Delphinus-Delphis Off the South-East Coast of Southern Africa}},
year = {1990}
}
@article{DerLingen2002,
abstract = {The diet of sardine Sardinops sagax in the southern Benguela was
investigated by microscopic examination of stomach contents. The
relative dietary importance of prey size and prey type was assessed by
calculating the carbon content of prey items. Sardine is an omnivorous
clupeoid, ingesting both phytoplankton and zooplankton, with the
relative importance of these two food types varying both spatially and
temporally. Stomach contents were numerically dominated by small prey
items, principally dinoflagellates, followed by crustacean eggs,
cyclopoid copepods, calanoid copepods and diatoms. Virtually all prey
items ingested by sardine were {\textless} 1.2 mm maximum dimension, the particle
size below which sardine only filter-feed. Despite the numerical
dominance by phytoplankton, zooplankton contributed the major portion to
sardine dietary carbon, small calanoid and cyclopoid copepods, anchovy
eggs and crustacean eggs being the primary prey types. These results
indicate that, like anchovy Engraulis capensis, sardine in the southern
Benguela are primarily zoophagous, and contrast with earlier dietary
studies on sardine in the region. However, the two species appear to
partition their prey on the basis of size; sardine consume small
zooplankton, whereas anchovy consume large zooplankton. This difference
has been observed in other upwelling ecosystems where the two genera
co-exist and is likely to contribute to the regime shifts observed
between sardine and anchovy.},
author = {der Lingen, C D},
issn = {0257-7615},
journal = {SOUTH AFRICAN JOURNAL OF MARINE SCIENCE-SUID-AFRIKAANSE TYDSKRIF VIR SEEWETENSKAP},
keywords = {feeding; phytoplankton; sardine; southern Benguela},
title = {{Diet of sardine Sardinops sagax in the southern Benguela upwelling ecosystem}},
year = {2002}
}
@article{Marais1990b,
abstract = {The chemical body composition of ten marine migrants and one freshwater fish species caught in Eastern Cape estuaries was determined. Fish species were divided into three categories, taking fat/energy content as the criterion. Two species of mullet and Elops machnata had a high fat/energy content ({\textgreater}8 kJ/g, {\textgreater}8{\%} fat), Pomatomus saltatrix, Liza tricuspidens, Diplodus sargus, Monodactylus falciformis, Labeo umbratus and Pomadasys commersonnii had medium values (5.5-8 kJ/g, 4-8{\%} fat) and Lichia amia and Argyrosomus hololepidotus low values ({\textless}5.5 kJ/g, {\textless}4{\%} fat). Statistically significant correlations between body constituents allows the determination of the energy and fat content of fish species from a simple moisture analysis},
author = {Marais, J F K},
journal = {South African Journal of Marine Science/Suid-Afrikaanse Tydskrif vir [S.AFR.J.MAR.SCI./S.-AFR.TYDSKR.SEEWET.]},
title = {{Body composition of ten marine migrants and one freshwater fish species caught in estuaries of the Eastern Cape, South Africa}},
year = {1990}
}
@article{Leroux1990a,
abstract = {The crab Carcinus maenas (L.) has an endemic distribution along the European Atlantic coastline but has been accidentally introduced into intertidal habitats in several parts of the world. Its discovery at the Cape Peninsula in South Africa is recorded here. The crab is well established in Table Bay harbour and the population is at least seven years old. From here it has spread 117 km north and 10 km south. Populations on the open coast are more recently established. The crab feeds on a wide range of intertidal animals, particularly the isopod Paridotea ungulata, limpets and polychaetes, but it is too early to make quantitative assessments of its impact on local intertidal communities. Initial predation experiments suggest that the barnacle Notomegabalanus algicola and the whelks Oxystele variegata and O. tigrina have no refuge in size or zonation from the crab. O. sinensis, of similar size to O. tigrina, is less vulnerable by virtue of its stronger shell. Its critical size (above which it is too large to be attacked) is correlated with crab size. Larger species, such as the mussels Choromytilus meridionalis and Aulacomya ater and the periwinkle Turbo sarmaticus, clearly achieve a refuge in size, but predation on their recruits may influence their abundance and size structure. C. maenas rejected only the urchin Parechinus angulosus as a source of food. While it is conceivable that shifts in community structure, selection for thicker shells in molluscs and localized extinction of certain species may occur at high crab densities, it is predicted that C. maenas will only have a significant impact in areas protected from wave action.},
author = {Leroux, P J and Branch, G M and Joska, M A P},
journal = {South African Journal Of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{On The Distribution, Diet And Possible Impact Of The Invasive European Shore Crab Carcinus-Maenas (L) Along The South-African Coast}},
year = {1990}
}
@article{STEINKE1993,
abstract = {Feeding of Sesarma meinerti de Man was studied in the laboratory and at$\backslash$nthe Beachwood Mangrove Reserve. n the field, mangrove leaf litter formed$\backslash$n74,6{\%} of the diet. Crabs showed a preference for leaves in the order:$\backslash$nyellow Bruguiera gymnorrhiza (L.) Lam. {\textgreater} yellow Avicennia marina$\backslash$n(Forssk.) Vierh. {\textgreater} green B. gymnorrhiza {\textgreater} green A. marina. In the$\backslash$nlaboratory, a positive correlation was observed between relative$\backslash$nconsumption and degree of decomposition. Crabs fed with both tannin and$\backslash$ntannin-free leaves showed a preference for the latter. Removal of B.$\backslash$ngymnorrhiza leaves by S. meinerti was high throughout the year, whereas$\backslash$ndry mass toss attributable to consumption varied according to season. It$\backslash$nis suggested that S. meinerti plays an important role in the degradation$\backslash$nof mangrove leaf litter.},
author = {STEINKE, T D and RAJH, A and HOLLAND, A J},
issn = {0257-7615},
journal = {SOUTH AFRICAN JOURNAL OF MARINE SCIENCE-SUID-AFRIKAANSE TYDSKRIF VIR SEEWETENSKAP},
title = {{THE FEEDING-BEHAVIOR OF THE RED MANGROVE CRAB SESARMA-MEINERTI-DE MAN, 1887 (CRUSTACEA, DECAPODA, GRAPSIDAE) AND ITS EFFECT ON THE DEGRADATION OF MANGROVE LEAF-LITTER}},
year = {1993}
}
@article{Smale1991g,
abstract = {The shark species Carcharhinus brachyurus, C. obscurus and Sphyrna zygaena, which attain more than 2 m when adult, occur in coastal waters of the Southern Cape as juveniles. They use these coastal waters as nursery areas and their relative abundance there varies through the year, possibly because of seasonally varying oceanographic conditions. Adult C. brachyurus occur in the same geographic area but usually deeper, although there is some overlap in juvenile and adult depth ranges. Adult C. obscurus prefer the warmer subtropical and tropical waters north of the study area. Adult S. zygaena are found over deep reefs at the edge of the continental shelf. There was considerable dietary overlap between the three species, the prey of all three being dominated by small schooling fish, such as pilchard, and squid. For the three species, animals of total length {\textgreater}2 m took larger prey, such as elasmobranchs, with greater frequency, although they continued to prey on squid and schooling fish. It is postulated that the nursery areas are in the study area because of the abundance of food there, and that avoidance of cannibalism would be enhanced by the use of such nurseries.},
author = {Smale, M J},
isbn = {0257-7615},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{Occurrence and Feeding of three Shark Species, {\textless}i{\textgreater} Carcharhinus brachyurus, C. obscurus {\textless}/i{\textgreater} and {\textless}i{\textgreater}Sphyrna zygaena{\textless}/i{\textgreater}, on the Eastern Cape Coast of South-Africa}},
year = {1991}
}
@article{Harwood1992a,
abstract = {Commercial fishermen in many parts of the world perceive marine mammals as competitors for a scarce resource: fish. The importance of predation by marine mammals can be evaluated crudely by considering the marine mammal as a rival fishery whose operation is constrained by physiology and energetic requirements, rather than by catch limits and gear restrictions. Evidence collected from grey seals Halichoerus grypus in the United Kingdom, southern elephant seals Mirounga leonina and beluga whales Delphinapterus leucas shows that published information on distribution, based on the location of breeding sites and sightings in shallow water, may give an unrealistic picture of foraging distribution. Studies of diet and distribution can give an impression of the static nature of the interaction between a marine mammal and the fishery. However, it is the dynamics of the interaction which are important. A critical question to be posed is how will the consumption of a particular prey species vary with its availability to the predator?},
author = {Harwood, J},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{Assessing the Competitive Effects of Marine Mammal Predation on Commercial Fisheries}},
year = {1992}
}
@article{Cockcroft1995c,
abstract = {Negative growth (shrinkage) at moulting in male and female West Coast rock lobster Jasus lalandii under field conditions is documented. A decline in growth rates of male rock lobsters in the]are 1980s/early 1990s is reflected in the steep rise in the frequency of zero and negative growth increments at moulting in four areas along the South African west coast. Shrinkage is considered to be a direct result of adverse environmental conditions and not a physiological mechanism evolved to overcome defined periods of hardship. Mean growth rates of male lobsters incorporating negative growth increments are presented and the implications for management discussed. Slow growth resulted in a decrease in female size at 50{\%} maturity in all four study areas, suggesting that juvenile as well as adult growth rates have been adversely affected.},
author = {Cockcroft, A C and Goosen, P C},
isbn = {0257-7615},
journal = {South African Journal of Marine Science - Suid - Afrikaanse Tydskrif Vir Seewetenskap},
keywords = {GROWTH/ CRABS},
title = {{Shrinkage at moulting in the rock lobster Jasus lalandii and associated changes in reproductive parameters}},
year = {1995}
}
@article{COCKCROFT1991,
abstract = {The offshore distribution and abundance of bottlenose dolphins Tursiops truncatus along the Natal South Coast was assessed aerially in May of 1989 and 1990. Unlike off the Natal North Coast and the Eastern Cape, a substantial proportion of sightings was made in water deeper than 30 m. This indicates that distribution on the South Coast may be different from that in other areas, possibly because of the presence there of offshore reefs, the continued degradation of the Natal inshore environment and the 1987 floods. There were probably fewer than 350 bottlenose dolphins off the South Coast, an apparent decline since the last survey of the area in 1985. Incidental captures of bottlenose dolphins on the South Coast average 24 animals per year, more than 7 per cent of the estimated maximum number. The consequences of this and also of the continued degradation of Natal's coastal zone are examined.},
author = {COCKCROFT, VG and ROSS, GJB and PEDDEMORS, VM},
issn = {0257-7615},
journal = {SOUTH AFRICAN JOURNAL OF MARINE SCIENCE-SUID-AFRIKAANSE TYDSKRIF VIR SEEWETENSKAP},
title = {{DISTRIBUTION AND STATUS OF BOTTLE-NOSED-DOLPHIN TURSIOPS-TRUNCATUS ON THE SOUTH COAST OF NATAL, SOUTH-AFRICA}},
year = {1991}
}
@article{Goosen1995a,
abstract = {This note reviews all moult increment information available for male West Coast rock lobster (published and unpublished) and collates these data into a single long-term growth record for each of the major subareas along the South African west coast. The data synthesized highlight the complexity of the growth information available for J. lalandii. The factors influencing direct comparisons of the data are discussed and the possible problems arising from the different techniques used during the study period are examined. The manuscript also incorporates increments of negative growth which were ignored previously. [References: 17]},
author = {Goosen, P C and Cockcroft, A C},
journal = {South African Journal of Marine Science Suid Afrikaanse Tydskrif vir Seewetenskap},
title = {{Mean annual growth increments for male west coast rock lobster jasus lalandii, 1969-1993}},
year = {1995}
}
@article{Shannon1998a,
abstract = {By means of modelling, the South African anchovy Engraulis capensis and
sardine Sardinops sagax are compared in terms of spawning, transport and
survival of young stages. As there is variability in the areas across
which adult anchovy and sardine spawn, two contrasting egg distritutions
were modelled for each species. Dissimilarities found between the two
species can be attributable to (i) differences in egg distributions, and
hence in the paths along which eggs and larvae are transported passively
by currents, (ii) differences in survival rates of eggs and larvae,
(iii) differences in duration of stages until swimming age and (iv)
differences in the duration of the spawning season. In the absence of
advective losses, more anchovy reached swimming age than did sardine.
However, anchovy spawned in regions that are more susceptible to
advective losses. Adult biomass levels were low in the years when
anchovy and sardine spawning was restricted to the South Coast. Also,
advective losses of both species were greater when spawning
distributions were contracted. Higher proportions of young reached the
West Coast when spawning extended over the South and up the West coasts.
Sardine accumulated farther west than anchovy. Anchovy eggs and larvae
were lost to advection mainly on the South Coast between Cape Agulhas
and Mossel Bay, whereas advective losses of sardine were restricted to
areas west of the Cape Peninsula.},
author = {Shannon, L J},
issn = {0257-7615},
journal = {SOUTH AFRICAN JOURNAL OF MARINE SCIENCE-SUID-AFRIKAANSE TYDSKRIF VIR SEEWETENSKAP},
title = {{Modelling environmental effects on the early life history of the South African anchovy and sardine: A comparative approach}},
year = {1998}
}
@article{Attwood1995e,
abstract = {A discrete, space-time, age-structured model of a fishery was developed to test the effect of marine reserves on yield of fish to recreational angling. The model was applied to three sympatric surf-zone species commonly targeted by shore anglers. Movement rates of white steenbras lithognathus lithognathus and galjoen Dichistius capensis, both of which are nomadic, were estimated from tag-recovery data. Because post-recruit blacktail Diplodus sargus capensis are resident, a coefficient of passive larval diffusion was estimated from drogue-separation rates. Marine reserves cannot increase the yield-per-recruit of white steenbras, although the spawner-biomass-per-recruit responds positively. A ''recruitment spawner-biomass'' function was applied to galjoen and blacktail. The yield of galjoen increased sharply as the size of area conserved increased. Small reserves, closely spaced, appeared to be optimal for blacktail. A range of reserve size and spacing combinations satisfied management criteria for the three fisheries. Because of the assumption of a uniform coastline, and the unknown extent of effort displacement by reserves, the results are too general to be of specific value, but they do suggest that management of multispecies fisheries by the use of marine reserves is a viable option. With the availability of site-specific data, the model can be applied to evaluate the effect of existing or proposed marine reserves.},
author = {Attwood, C G and Bennett, B A},
journal = {South African Journal of Marine Science   Suid   Afrikaanse Tydskrif Vir Seewetenskap},
keywords = {STEENBRAS LITHOGNATHUS-LITHOGNATHUS/GALJOEN CORACI},
title = {{Modelling the effect of marine reserves on the recreational shore-fishery of the South-Western Cape, South Africa}},
year = {1995}
}
@article{Abello1990a,
abstract = {The distribution patterns of the benthic stomatopod crustacean Pterygosquilla armata capensis were studied off southern Namibia between latitudes 23 and 30-degrees-S and between about 50 and 500 m depth. Whereas south of 28-degrees-S and off the South African coast this species is mainly a coastal animal distributed on soft substrata at depths shallower than 150-200 m, the maximum densities in Namibia occur in much deeper waters (200-300 m). In winter, maximum relative densities of the species are found shallower ({\textless} 200 m depth) than in summer (200-300 m). This density shift is thought to be related to the fact that, in summer, the main upwelling period has passed and inshore oxygen levels on the bottom decrease to highly hypoxic values. The species avoids areas shallower than 200 m north of 27-degrees-S, the area where oxygen depletion is strongest during all seasons. Oxygen levels appear to be critical to the inshore distribution of the species, whereas temperatures of {\textless} 10-11-degrees-C seem to limit its distribution towards deeper waters. Highest abundances occur on sediments with 10-40 per cent silt-clay content. Population size structure did not show any apparent peaks which could be related to age cohorts. Males attained larger size than females.},
author = {Abello, P and Macpherson, E},
journal = {South African Journal of Marine Science-Suid-Afrikaanse Tydskrif Vir Seewetenskap},
title = {{Influence of Environmental-Conditions on the Distribution of Pterygosquilla-Armata Capensis (Crustacea, Stomatopoda) Off Namibia}},
year = {1990}
}
@article{AMeadCLGriffithsGMBranchCDMcQuaidLKBlameyJJBolton2013,
abstract = {Coastal  ecosystems  are  highly  vulnerable  to  human-mediated  drivers  of  global  change  because  they  are  located  $\backslash$r$\backslash$nat  the  land–ocean  interface  and  often  host  centres  of  urbanisation  and  development.  The  South  African  coastline  $\backslash$r$\backslash$ncomprises several distinct coastal ecoregions that support a wide range of coastal (inshore) ecosystems, including $\backslash$r$\backslash$nrocky,  sandy  and  mixed  shores,  kelp  beds,  estuaries  and  seagrass  communities.  A  growing  body  of  evidence  $\backslash$r$\backslash$nindicates  that  local  air  and  sea  temperatures,  wind  patterns,  ocean  current  speed  and  upwelling  regimes  are  all  $\backslash$r$\backslash$nbeing  affected  by  human-mediated  climate  change.  In  addition,  anthropogenic  activities,  such  as  shipping  $\backslash$r$\backslash$n(introducing  coastal  bioinvasives),  exploitation  of  coastal  marine  resources,  industry  (releasing  pollutants)  and  $\backslash$r$\backslash$nurban  development,  act  synergistically  with  climate  change  to  place  pressure  on  coastal  ecosystems  and  their  $\backslash$r$\backslash$nbiota.  The  aim  of  this  review  was  primarily  to  synthesise  and  update  research  into  causes  of  direct  and  indirect  $\backslash$r$\backslash$nhuman-mediated  global  change  and  their  effects  on  South  African  coastal  systems.  It  incorporates  both  historic  $\backslash$r$\backslash$nand  the  latest  regional  research  on  climate  change  and  anthropogenic  threats  across  the  ecosystems  listed  $\backslash$r$\backslash$nabove, much of which was supported by the South African Network for Coastal and Oceanic Research (SANCOR), $\backslash$r$\backslash$nspecifically  the  SEAChange  programme  in  recent  years.  It  is  evident  that  all  these  ecosystems  are  vulnerable  to  $\backslash$r$\backslash$nall  the  drivers  considered,  albeit  to  differing  degrees,  depending  on  their  location  on  the  coast.  Whereas  some  $\backslash$r$\backslash$nbioinvasives  have  had  a  dramatic  impact  on  rocky  shore  systems  on  the  West  Coast,  their  impact  has  been  $\backslash$r$\backslash$nmoderate on the South Coast and minimal on the East Coast; exploitation shows the reverse pattern. Furthermore, $\backslash$r$\backslash$nthe impacts of human-mediated drivers on coastal ecosystems are synergistic. Of major interest is the fact that the $\backslash$r$\backslash$nWest Coast and parts of the South Coast are exhibiting cooling trends in offshore sea surface temperatures, rather $\backslash$r$\backslash$nthan  warming.  Correspondingly,  a  geographical  spread  of  organisms  associated  with  West  and  South-West  Coast  $\backslash$r$\backslash$nrocky shores and kelp beds has tended to be eastwards around Cape Point, rather than northwards along the West $\backslash$r$\backslash$nCoast  as  would  have  been  expected  with  warming  sea  temperatures.  Overall,  significant  progress  has  been  made  $\backslash$r$\backslash$ntoward  a  better  understanding  of  the  combined  pressures  on  each  ecosystem  and  knowledge  gaps  have  been  $\backslash$r$\backslash$nidentified, thus helping to direct future research themes. },
author = {{A Mead, C L Griffiths, G M Branch, C D McQuaid, L K Blamey, J J Bolton}, R J and {Anderson, F Dufois, M Rouault, P W Froneman, A K Whitfield, L R Harris}, R and Nel, D Pillay {\&} J B Adams},
doi = {http://dx.doi.org/10.2989/1814232X.2013.83014},
journal = {Full Terms {\&} Conditions of access and use can be found at http://www.tandfonline.com/action/journalInformation?journalCode=tams20 Download by:  [University of Cambridge] Date:  23 May 2017, At: 07:18 African Journal of Marine Science},
title = {{Human-mediated drivers of change  — impacts on coastal ecosystems and marine biota of South Africa}},
year = {2013}
}
