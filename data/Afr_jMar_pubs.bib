@article{Pistorius2011b,
abstract = {Southern elephant seal Mirounga leonina numbers declined precipitously throughout most of their circumpolar distribution since the 1950s. A long-term intensive demographic programme was initiated in 1983 on the relatively small population of southern elephant seals at sub-Antarctic Marion Island in an attempt to identify causative mechanisms associated with this decline. Weaned pups have been tagged annually since 1983, and this has produced a large number of individuals of known identity. A regular resighting programme yielded a mark-recapture dataset that has been subjected to numerous survivalbased models. This ongoing programme produced a substantial body of scientific literature on population growth patterns, vital rates (survival and fecundity) and population regulation in southern elephant seals, which are reviewed in this synthesis. We briefly describe the analytical framework common to much of the demographic research, highlight important conclusions concerning population regulation of elephant seals at Marion Island, and discuss priorities for future research.},
author = {Pistorius, P A and de Bruyn, P J N and Bester, Marth{\'{a}}n N.},
doi = {10.2989/1814232X.2011.637357},
issn = {1814-232X},
journal = {African Journal of Marine Science},
title = {{Population dynamics of southern elephant seals: a synthesis of three decades of demographic research at Marion Island}},
year = {2011}
}
@article{Salmeron2015,
abstract = {Fish stocks associated with seamounts may be particularly susceptible to$\backslash$noverexploitation. From January to July 2001, the Spanish Oceanographic$\backslash$nInstitute (IEO) conducted an experimental fishing survey entitled$\backslash$n`Palguinea-2001' on the seamounts of the Sierra Leone Rise. Beryx$\backslash$nsplendens (commonly called alfonsino) is the main commercial demersal$\backslash$nfish associated with this area. The aim of this study was to investigate$\backslash$nthe effects of a demersal longline fishery targeting B. splendens on the$\backslash$npreviously unexploited small Machucambo Seamount over a short$\backslash$ntime-scale, and also to consider trends in pooled catch rate at another$\backslash$nfour seamounts. During 110 fishing days at Machucambo, a total catch of$\backslash$n207 tonnes of B. splendens was taken with a fishing effort of 1 309 070$\backslash$nhooks. A spectral analysis and red-noise spectra procedure (REDFIT)$\backslash$nalgorithm was used to identify the red-noise spectrum from the gaps in$\backslash$nthe observed time-series of catch per unit effort by weight. Our results$\backslash$nshow the potential impact of longline fishing pressure on an unexploited$\backslash$necosystem - after approximately 50 fishing days, the stock appeared to$\backslash$ndecline substantially, as reflected by a marked drop in catch per unit$\backslash$neffort. The apparent rapid decline of the stock might be related to the$\backslash$nsmall size and the virgin state of the Sierra Leone seamounts. The$\backslash$nresults could be extrapolated to similar small seamounts elsewhere.},
author = {Salmer{\'{o}}n, F. and B{\'{a}}ez, J. C. and Mac{\'{i}}as, D. and Fernandez-Peralta, L. and Ramos, A.},
doi = {10.2989/1814232X.2015.1085902},
issn = {18142338},
journal = {African Journal of Marine Science},
keywords = {CPUE,alfonsino,demersal fishery,fisheries management,longline},
title = {{Rapid fish stock depletion in previously unexploited seamounts: the case of Beryx splendens from the Sierra Leone Rise (Gulf of Guinea)}},
year = {2015}
}
@article{Gotz2009f,
abstract = {Exploitation of temperate reef fish not only affects the target species but potentially changes the composition of reef fish assemblages . This study investigated the effect of fishing on the ichthyofaunal community at protected and exploited sites around the Goukamma Marine Protected Area (MPA) on the South African temperate south coast. Abundance and size frequency data from 273 standardised angling and 44 underwater visual census sites were analysed with generalised linear modelling and multivariate techniques to separate the effects of exploitation, habitat, time of day and season. Ichthyofaunal communities differed significantly between protected and exploited sites, which is likely a result of differential fishing pressure. Abundance and size of roman Chrysoblephus laticeps, the dominant species of the local linefishery, were significantly lower at exploited sites. Non-target species with considerable dietary overlap with roman were significantly less abundant inside the MPA, probably due to interspecific competition, whereas species whose diets differed to that of roman were more abundant at protected sites. The results show that fishing can have an influence on the composition of target and non-target species and that these effects need to be considered in fisheries management and conservation planning. {\textcopyright} NISC (Pty) Ltd.},
author = {G{\"{o}}tz, A and Kerwath, SE and Attwood, CG and Sauer, WHH},
doi = {10.2989/AJMS.2009.31.2.12.884},
issn = {1814-232X},
journal = {African Journal of Marine Science},
title = {{Effects of fishing on a temperate reef community in South Africa 1: ichthyofauna}},
year = {2009}
}
