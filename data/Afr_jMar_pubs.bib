@article{Zoutendyk1988,
abstract = {In order to estimate the absorption rate of carbon by the palinurid lobster Jasus lalandii the net quantity of ingested carbon and gross carbon content of faeces must be known. Experimental animals were selected for consistent feeding and subsequently starved for 72 h. Ingested carbon was established by determining the carbon content of the prey mussel Choromytilus meridionalis under controlled aquarium conditions through five eating/defaecating cycles. The losses of particulate and dissolved organic carbon due to messy feeding were measured and amounted to 20,1 per cent of the food presented. Of the net carbon ingested, 20,5 per cent was voided as faeces, thereby giving a mean absorption efficiency of 79,5 per cent (n = 15, SD = 4,6{\%}). J. lalandii , as a top carnivore in the inshore Benguela ecosystem, is therefore highly efficient in the utilization of ingested carbon$\backslash$n},
author = {Zoutendyk, P.},
doi = {10.2989/025776188784480591},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Feeding, defaecation and absorption efficiency in the Cape rock lobster Jasus lalandii}},
year = {1988}
}
@article{Marais1990,
abstract = {The chemical body composition of ten marine migrants and one freshwater fish species caught in Eastern Cape estuaries was determined. Fish species were divided into three categories, taking fat/energy content as the criterion. Two species of mullet and Elops machnata had a high fat/energy content ({\textgreater}8 kJ{\textperiodcentered}g−1; {\textgreater}8{\%} fat), Pomatomus saltatrix, Liza tricuspidens, Diplodus sargus, Monodactylus falciformis, Labeo umbratus and Pomadasys commersonnii had medium values (5,5–8 kJ{\textperiodcentered}g−1; 4–8{\%} fat) and Lichia amia and Argyrosomus hololepidotus low values ({\textless}5,5 kJ{\textperiodcentered}g−1; {\textless}4{\%} fat). Statistically significant correlations between body constituents allows the determination of the energy and fat content of fish species from a simple moisture analysis.},
author = {Marais, J F K},
doi = {10.2989/025776190784378844},
issn = {0257-7615},
journal = {South African Journal of Marine Science},
title = {{Body composition of ten marine migrants and one freshwater fish species caught in estuaries of the Eastern Cape, South Africa}},
year = {1990}
}
