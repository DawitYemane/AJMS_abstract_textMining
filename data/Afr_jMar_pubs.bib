@article{Kawasaki1992,
abstract = {A comprehensive and systematic review of 10 years' study on global fluctuations of c1upeoid populations is presented and earlier ideas funher developed. Historically, sardine and herring have undergone fluctuations of very large amplitude over long periods in a cyclic manner. Such a fluctuation, called Subtype IB, could not have evolved in any other than a marine environment. Sardine and herring have shown concomitant cycles of variation in biomass, but the two cycles are completely out of phase with each other. Sardine dominate the temperate pelagic fish community and changes in its abundance are responsible for structural changes of the community. 'n Omvattende en sistematiese oorsig van 10 jaar se studie oor wereldwye skommelinge in k1upeo'iede bevol-kings word aangebied en vroeere idees verder ontwikkel. Histories het sardyn en haring skommelinge van baie groot amplitude oor lang tydperke op 'n sikliese wyse ondergaan. Sulke skommelinge, Subtipe IB genoem. kon nie in 'n omgewing anders as 'n mariene een evolueer het nie. Sardyn en haring het saamlopende siklusse van biomassaskommeling venoon, maar die twee siklusse is heeltemal uit fase met mekaar. Sardyn oorheers die pelagiese visgemeenskap van gematigde streke, en wisselinge in hul talrykheid is verantwoordelik vir die struk-tuurveranderinge van die gemeenskap.},
author = {Kawasaki, T.},
doi = {10.2989/02577619209504748},
isbn = {0257-7615},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Mechanisms governing fluctuations in pelagic fish populations}},
year = {1992}
}
@article{Cliff1989a,
abstract = {A total of 591 great white sharks Carcharodon carcharias was caught between 1974 and 1988 in the gill nets which are maintained along the Natal coast to protect bathers from shark attack. The species represented 2,7 per cent of the total catch of sharks for the period 1978–1988 and the mean catch rate was 1,0 shark{\textperiodcentered}km-net−1{\textperiodcentered}year−1. Specimens ranged in size from 131 to 348 cm precaudal length, with a mode of 206–210 cm for females and 216–220 cm for males. None was mature, and the sex ratio was 1 male to 1,4 females. Catch rates were highest in the southernmost nets, with a smaller peak in the extreme north. Catches peaked in midwinter, those in the warmer, northern region lagging 1–2 months behind those in the cooler southern waters. The relationship between catches and water clarity and temperature was tenuous. Elasmobranchs were the most important prey item, occurring in 41,1 per cent of stomachs containing food, followed by teleosts at 34,7 per cent and marine mammals at 29,0 per cent, although marine ...},
author = {Cliff, G. and Dudley, S. F. J. and Davis, B.},
doi = {10.2989/02577618909504556},
isbn = {0257-7615},
issn = {0257-7615},
journal = {South African Journal of Marine Science},
title = {{Sharks caught in the protective gill nets off Natal, South Africa. 2. The great white shark {\textless}i{\textgreater}Carcharodon carcharias{\textless}/i{\textgreater} (Linnaeus)}},
year = {1989}
}
@article{Pitcher1992,
abstract = {The overall spectrum of phytoplankton heterogeneity within the southern Benguela is classified and presented on the basis of spatial and temporal scales, ranging from the macro- to the microscale. State variables in terms of floristic and biomass changes and rate variables in terms of physiological changes are reviewed and related to the physical and biological mechanisms important to their generation and maintenance. Biological and physical processes acting alone and in concert give rise to the spatial and temporal patterns of phytoplankton distribution, making categorization by causative mechanisms both difficult and artificial. Nevertheless, all scales and types of patchiness in the southern Benguela are strongly influenced by the hydrodynamic process of upwelling, both through redistribution of existing biological variability and through the creation of spatial variability in the richness and suitability of the physical and chemical environment. Physical influences override biotic interactions during upwelling whereas, with the onset of stratification, physical processes become less dynamic, allowing biological and chemical factors to become the major determinants of phytoplankton variability. These observations are spatially confirmed, with physical forcing dominant close to the source of upwelling and biological influences more important downstream of upwelling.},
author = {Pitcher, G. C. and Brown, P. C. and Mitchell-Innes, B. A.},
doi = {10.2989/02577619209504717},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Spatio-temporal variability of phytoplankton in the southern Benguela upwelling system}},
year = {1992}
}
@article{Butterworth1990,
abstract = {The arbitrariness of Virtual Population Analysis (VPA) solutions in the absence of objectively based procedures to tune both oldest-age and most-recent-year terminal fishing mortalities is demonstrated. The formalism for tuning the latter set of mortalities with effort data from one or more participating fleets is presented, and procedures for estimating the variance of the resultant solution for Laurec-Shepherd tuning are proposed. These methods are applied to the stock of Cape horse mackerel off Namibia. Overall biomass trends are poorly determined. A multiple-fleet disaggregated-data VPA for the Polish and Soviet effort series estimates the 1989 F 0.1 Total Allowable Catch (TAC) to be a little in excess of 700 000 tons, with an associated negatively biased 95-per-cent confidence interval estimate of [220; 3 500] thousand tons. This enormous imprecision suggests that F 0.1 TAC estimation based on a VPA should be abandoned as the basis for management recommendations for the resource. A replacement-yield harvesting strategy is advocated as an interim measure; the associated TAC estimated would be 517 (SE 19) thousand tons using data on catch per unit effort and estimated average biomass from VPA. A status-quo F harvesting strategy, and the possibilities for direct surveys of the resource, merit consideration.$\backslash$nThe arbitrariness of Virtual Population Analysis (VPA) solutions in the absence of objectively based procedures to tune both oldest-age and most-recent-year terminal fishing mortalities is demonstrated. The formalism for tuning the latter set of mortalities with effort data from one or more participating fleets is presented, and procedures for estimating the variance of the resultant solution for Laurec-Shepherd tuning are proposed. These methods are applied to the stock of Cape horse mackerel off Namibia. Overall biomass trends are poorly determined. A multiple-fleet disaggregated-data VPA for the Polish and Soviet effort series estimates the 1989 F 0.1 Total Allowable Catch (TAC) to be a little in excess of 700 000 tons, with an associated negatively biased 95-per-cent confidence interval estimate of [220; 3 500] thousand tons. This enormous imprecision suggests that F 0.1 TAC estimation based on a VPA should be abandoned as the basis for management recommendations for the resource. A replacement-yield harvesting strategy is advocated as an interim measure; the associated TAC estimated would be 517 (SE 19) thousand tons using data on catch per unit effort and estimated average biomass from VPA. A status-quo F harvesting strategy, and the possibilities for direct surveys of the resource, merit consideration.},
author = {Butterworth, D. S. and Hughes, G. and Strumpfer, F.},
doi = {10.2989/025776190784378718},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{VPA with ad hoc tuning: Implementation for disaggregated fleet data, variance estimation, and application to the Namibian stock of cape horse mackerel Trachurus trachurus capensis}},
year = {1990}
}
@article{Waldron1992b,
abstract = {Thermal infra-red satellite imagery and ship-borne studies of 15N stable isotopes are used to provide a first estimate of the quantity of carbon exported from the Benguela upwelling system during summer 1983/84. From satellite imagery, a region where cold water of upwelling origin was present on a quasi-permanent basis and a more offshore region where the presence of such water was intermittent were designated. New production inside the 200 m isobath was excluded from the calculations because an unknown proportion of it may be recycled within the continental shelf system and would not therefore constitute export production and hence potential sequestration. Total production can be apportioned between new and regenerated production depending on the relative utilization of NO3-N or NH4-N and urea respectively. New production generally results from the input of NO3-N to the euphotic zone via upwelling and/or turbulent diffusion and is the only portion of the total production which, in the long term, constitutes a carbon sink. This principle is modified to address the problem of possible new-production recycling on the continental shelf on short time-scales. Nitrate-uptake experiments conducted in the euphotic zone of aged upwelled and filament water throughout the Benguela yielded values of new production, and the mean of these data was applied to the appropriate areas identified by satellite imagery. New production off the continental shelf during summer 1983/84 attributable to upwelling-derived water was 2.14 x 10{\^{}}13 ± 1.77 x 10{\^{}}13 g. An attempt is made to place this value in the context of new-production pathways in the Benguela.},
author = {Waldron, H. N. and Probyn, T. A. and Lutjeharms, J. R.E. and Shillington, F. A.},
doi = {10.2989/02577619209504712},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Carbon export associated with the Benguela upwelling system}},
year = {1992}
}
@article{Bailey1987,
abstract = {Regeneration of nutrients from Benguela sediments taken between Llideritz. and Walker Bay, on the leeward side of upwelling centres, was studied by means of two methods. First, porewater samples were taken from sediment cores and analysed for nutrients. The observed concentration gradient was used to estimate the potential flux by diffusion of nutrients across the sediment/water interface. Second, intact sediment cores were incubated at the temperatures and concentrations of dissolved oxygen found at the interface, and the rate of increase of nutrients in the overlying water was monitored. The systems where samples were taken may be classified as semi-closed, as there is preferential settling of particulate organic carbon and regeneration of nutrients from the sediments. Here, the nutrient concentrations in bottom waters exceeded those in upwelling source water by up to 100 per cent, probably because of the regeneration process. More stable water-column conditions on the leeward side of upwelling centres may inhibit exchange of nutrients between boltom waters and the euphotic zone: implications of this possibility are discussed. A mechanism is proposed as to thc manner in which nutrient-rich boltom water from 5t Helena Bay may be advected southwards during current reversals associated with coastal trapped waves. This mechanism would prime the upwelling centre at Cape Columbine and could result in advection of nutrients back into 5t Helena Bay euphotic zone waters.},
author = {Bailey, G. W.},
doi = {10.2989/025776187784522478},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{The role of regeneration from the sediments in the supply of nutrients to the euphotic zone in the Southern Benguela system}},
year = {1987}
}
@article{Walker1991a,
abstract = {Phytoplankton species distribution and production in relation to hydrology$\backslash$nwere examined in St. Helena Bay in the southern Benguela system during$\backslash$nDecember 1987. The presence of a red-tide bloom of the dinoflagellate$\backslash$nCeratium furca provided the opportunity to examine the conditions$\backslash$nrequired for the development of such an event. Hydrographic parameters$\backslash$nindicated the intrusion of warm oceanic water adjacent to St. Helena$\backslash$nBay whereas, within the Bay, aged upwelled water was evident following$\backslash$nrelaxation of upwelling and stabilization of the water column. C.$\backslash$nfurca was recorded in surface waters at a concentration of 1,3 x$\backslash$n10 super(6) cells/l and productivity estimates exceeded 50 mgC multiplied$\backslash$nby m super(-3) multiplied by h super(-1). Productivity indices were$\backslash$nlowest within the red-tide bloom, indicating that the dominance of$\backslash$nC. furca cannot be explained by rapid growth. The bloom appear rather$\backslash$nto represent the accumulation of slow-growing ungrazed populations$\backslash$nsubject to concentration by physical entrainment and vertical migration.$\backslash$nPhytoplankton succession is less likely to be disrupted in St. Helena$\backslash$nBay because water masses are retained there for longer periods than$\backslash$noutside the Bay. The prevalence of late successional forms such as$\backslash$ndinoflagellates is therefore more likely within the Bay},
author = {Walker, D. R. and Pitcher, G. C.},
doi = {10.2989/02577619109504620},
isbn = {0257761910950},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{The dynamics of phytoplankton populations, including a red-tide bloom, during a quiescent period in St Helena Bay, South Africa}},
year = {1991}
}
@article{Verity1998,
abstract = {The conceptual framework for quantitative marine ecology is attributable$\backslash$nto Victor Hensen (1887), who proposed that quantitative studies of$\backslash$nplant and animal production in the sea would permit predictions of$\backslash$nannual fish yields. Hensen was strongly influenced by concurrent$\backslash$nconceptual developments in agriculture, in which crop production$\backslash$nwas being predicted from knowledge of physiology and its relationship$\backslash$nto environmental variables. As fish were being "harvested" by man,$\backslash$nit was argued that relationships similar to agriculture existed between$\backslash$nprimary production and fish yield. Thus was born the notion that$\backslash$npelagic ecosystems were structured from the "bottom-up", or resource-limited.$\backslash$nSubsequent refinements argued that, from basic knowledge of how vertical$\backslash$nmixing regulates primary production, and assuming certain features$\backslash$nof food-chain length and efficiency, one can estimate fish yields.$\backslash$nFundamental to these arguments are assumptions concerning resource$\backslash$nlimitation which appear to be uncertain as generic marine pelagic$\backslash$ncharacteristics, primarily that trophic levels are nutrient/food$\backslash$nlimited and respond to increased resource availability by elevated$\backslash$nstanding stocks. Whereas this conceptual model explains certain features$\backslash$nof energy flow, it fails to describe how marine pelagic foodwebs$\backslash$nare structured and why they function as they do. Rather, it appears$\backslash$nthat certain taxa are better than others at integrating their environments$\backslash$nand regulating the flux of materials through the foodweb, and that$\backslash$npredation is as important as resource limitation. There appears to$\backslash$nbe a distinct need in pelagic research to focus on predation, not$\backslash$nas a rate process so much as a mechanism responsible for organism$\backslash$nbehaviour, morphology, life history and community structure},
author = {Verity, Peter G},
doi = {10.2989/025776198784126962},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Why is relating plankton community structure to pelagic production so problematic ?}},
year = {1998}
}
@article{Barkai1986,
abstract = {The crop and stomach contents of Haliotis midae were determined at two study sites (on the south-western and western Cape coasts) in both summer and winter. A total of 18 species of algae was recorded in the diet, the most important being kelp Ecklonia maxima (56 per cent by volume) and Plocamium spp. (21 per cent). There was little size-related variation in diet over a range of shell width of 65?145 mm, except that Ulva spp. was of significantly greater importance in smaller specimens. The proportion of kelp consumed was higher on the West Coast than on the South Coast (66 vs 46 per cent), but Plocamium spp. was more important on the South Coast (39 vs 3 per cent of diet). I n summer more than 80 per cent of the samples contained these two species, but the winter diet was more varied at both sites. A comparison of diet and availability of food items indicated that some are selected for and others avoided.$\backslash$nThe crop and stomach contents of Haliotis midae were determined at two study sites (on the south-western and western Cape coasts) in both summer and winter. A total of 18 species of algae was recorded in the diet, the most important being kelp Ecklonia maxima (56 per cent by volume) and Plocamium spp. (21 per cent). There was little size-related variation in diet over a range of shell width of 65?145 mm, except that Ulva spp. was of significantly greater importance in smaller specimens. The proportion of kelp consumed was higher on the West Coast than on the South Coast (66 vs 46 per cent), but Plocamium spp. was more important on the South Coast (39 vs 3 per cent of diet). I n summer more than 80 per cent of the samples contained these two species, but the winter diet was more varied at both sites. A comparison of diet and availability of food items indicated that some are selected for and others avoided.},
author = {Barkai, R. and Griffiths, C. L.},
doi = {10.2989/025776186784461891},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Diet of the South African abalone Haliotis midae}},
year = {1986}
}
@article{Pillay1989,
abstract = {A closed system, supplied with water from the I ndlan Ocean and stocked with the prawn Penaeus monodon, was used to identify and study the cultural characteristics of the associated marine nitrifying bacteria. This system provided the ideal environment for the development of nitrifying bacteria, which was monitored by assaying for the oxidation of either ammonia or nitrite. As these organisms grow very slowly, enrichment and fed-batch cultures were prepared to obtain pure cultures. The nitrifying bacteria arechemolithotrophs and can only be identified by observing their arrangement of internal or cytomembranes with the aid of the transmission electron microscope. All five listed genera of marine nitrifying bacteria were observed, namely Nitrosomonas and Nirrosococcus, which oxidize ammonia to nitrite, and Nitrobacrer, Nirrococcus and Nirrospina, which oxidize nitrite to nitrate. Nirrosomonas europaea and Nirrobacler winogradskyi represented the dominant organisms in enrichment cultures and were also obtained in pure culture. Examination of the colonial morphology indicated the lack of distinct characteristics that could aid in their identification. A simple, effective method of observing the growth of ammonia-oxidizing bacteria microscopically, based on the breakdown of magnesium ammonium phosphate, is also described. 'n Geslote stelsel, voorsien van water uit die Indiese Oseaan en met die garnaal Penaeus monodon daarin, is gebruik ten einde identifikasie en die kultuureienskappe van die mariene nitrifiserende bakteriee daarmee verbonde, te bestudeer. Hierdie stelsel bied die ideale omgewing vir die ontwikkeling van nitrifiserende bakteriee, wat gemonitor is deurte toets of ammoniak of nitriet geoksideer word. Aangesien hierdie organismes baie stadig groei, is verrykingskulture berei om reinkulture te bekom. Die nitrifiserende bakteriee is chemolitotrofe en kan slegs geYdentifiseer word deur waarneming van die rangskikking van die interne of sitomembrane met behulp van die transmissie-elektronmikroskoop. AI vyf genera van mariene nitrifiserende bakteriee tot dusver beskryf, is waargeneem nl. Nilrosomonas en Nilrosococcus wat ammoniak na nitriet oksideer, en Nirrobacrer, Nilrococcus en Nirrospina wat nitriet na nitraat oksideer. Nilrosomonas europaea en Nilrobacler winogradskyi was die dominante organismes in verrykingskulture en is ook in reinkultuur verkry. 'n Ondersoek van die kolonie-morfologie het 'n gebrek aan kenmerkende eienskappe, waardeur hul identifikasie vergemaklik sou word, getoon. 'n Eenvoudige, dog effektiewe, metode vir die mikroskopiese waarneming van groei van ammoniak-oksiderende bakteriee wat op die afbreek van magnesiumammonium-fosfaat berus, word ook beskryf.},
author = {Pillay, B. and Roth, G. and Oellermann, R. A.},
doi = {10.2989/02577618909504573},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Cultural characteristics and identification of marine nitrifying bacteria from a closed prawn-culture system in Durban}},
year = {1989}
}
@article{Cruickshank1990,
abstract = {Qualitative acoustic surveys of the anchovy Engraulis capensis off Namibia were undertaken between 1978/79 and 1985. Near-monthly maps of shoal distribution are presented for each year which covered the summer spawning and the winter feeding seasons. Distribution densities are ranked according to three selected grades. The data are discussed in relation to the oceanography of the region as well as to previously documented spawning and larval distributions. Conclusions are drawn which may help in understanding the migration patterns of anchovy off Namibia. Adults occupy winter feeding grounds in the north and move out to the spawning grounds between the Cunene River and Hollams Bird Island in summer. Post-larvae are transported passively to the nursery areas where they feed and develop. Juveniles move north in autumn to the preferred feeding grounds around Walvis Bay, where they enter the fishery as recruits in winter. At the end of winter, they join the adults in the north. The inferred pattern would be advantageous to both feeding adults and juvenile anchovy, while also avoiding competition between the two components for food resources. This fits classical theory, but there are limitations to the data.},
author = {Cruickshank, R. A.},
doi = {10.2989/025776190784378691},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Anchovy distribution off Namibia deduced from acoustic surveys with an interpretation of migration by adults and recruits}},
year = {1990}
}
@article{Wainman1987,
abstract = {Four current-meter moorings placed in False Bay between February and May 1985 revealed a tendency towards cyclonic motion, but with poleward flow along the western boundary. Data show periodic acceleration of northward air flow associated with south-easterly wind events during ridging of the South Atlantic high pressure cell round the continent. A clear pattern of cooling and warming of water at the current-meter sites was observed. There was a suggestion of subsidence of warm water along the western boundary when the wind relaxed.},
author = {Wainman, C. K. and Polito, A. and Nelson, G.},
doi = {10.2989/025776187784522423},
isbn = {ISSN: 0257-7615},
issn = {0257-7615},
journal = {South African Journal of Marine Science},
title = {{Winds and subsurface currents in the False Bay region, South Africa}},
year = {1987}
}
@article{Melville-Smith1988,
abstract = {Commercial catch statistics for red crab Geryon maritae caught off South West Africa (Namibia) since 1980 and some historical data for the previous four years showed that crabs were much smaller in the northern areas than in the south. Catch per unif effort (cpue) was aseasonal and unaffected by soaking times of 24-120 h.},
author = {Melville-Smith, R.},
