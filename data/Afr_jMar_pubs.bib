journal = {South African Journal of Marine Science},
keywords = {undefined},
pmid = {5494206},
title = {{Fishery characteristics and abundance estimates of the mangrove crab Scylla serrata in southern Tanzania and northern Mocambique - South African Journal of Marine Science}},
year = {2002}
}
