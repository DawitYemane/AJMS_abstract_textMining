@article{Marais1990a,
abstract = {The chemical body composition of ten marine migrants and one freshwater fish species caught in Eastern Cape estuaries was determined. Fish species were divided into three categories, taking fat/energy content as the criterion. 1$\backslash$vo species of mullet and Elops machnata had a high fat/energy content ({\textgreater}8 kJ{\textperiodcentered}g-'; {\textgreater}8{\%} fat), Pomatomus sa/tatrix, Liza tricuspidens, Diplodus sargus, Monadacty/us fa/cifarmis, Labea umbratus and Pamadasys commersannii had medium values (5,5- 8 kJ{\textperiodcentered} go'; 4-8{\%} fat) and Lichia amia and Argyrasamus halalepidotus low values «5,5 kJ{\textperiodcentered}g-'; {\textless}4{\%} fat). Statistically significant correlations between body constituents allows the determination of the energy and fat content of fish species from a simple moisture analysis.},
