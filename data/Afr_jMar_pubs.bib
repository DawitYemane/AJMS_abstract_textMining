journal = {South African Journal of Marine Science},
title = {{Batch fecundity of pilchard sardinops ocellatus in the northern benguela system estimated from hydrated oocytes}},
year = {1988}
}
@article{Wilson1992,
abstract = {Attempts to use seabirds as monitors of prey stocks by examination of mortality of adult seabirds, the number of nesting attempts, chick growth or prey composition from stomach samples are often unsuccessful because these parameters are affected by many different environmental variables. In an attempt to reduce the variables, a measure of seabird catch per unit effort has been proposed. Advances in technology now make it possible to determine seabird catch per unit effort very accurately. The position of seabirds at sea can be determined via an externally attached dead-reckoning unit. Quantification ot where, when and how much prey is caught is determined by a complementary autonomic unit residing in the bird's stomach. From this equipment, the spatial distribution of seabird prey can be monitored. In addition, environmental sensors, fitted to the dead-reckoning unit, can give information on the oceanographic variables along the foraging track of the seabird.},
author = {Wilson, R. P.},
doi = {10.2989/02577619209504752},
isbn = {0257-7615},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Environmental monitoring with seabirds: Do we need additional technology?}},
year = {1992}
}
@article{Painting1992,
abstract = {During the 1980's, there was a major shift in the understanding of$\backslash$nthe structure of marine foodwebs. As a result, the microbial loop$\backslash$nhas been incorporated into the classical concept of the planktonic$\backslash$nfood chain. Heterotrophic bacteria and Protozoa have been shown to$\backslash$nbe important components of the plankton biomass in many parts of$\backslash$nthe world's oceans, and their role in the trophic dynamics of pelagic$\backslash$nfoodwebs has been studied intensively. In the southern Benguela,$\backslash$nfield, laboratory and modelling studies have been combined to calculate$\backslash$ncarbon and nitrogen fluxes through the microheterotrophic portion$\backslash$nof the pelagic foodweb. A size-based simulation model incorporating$\backslash$nrecent hypotheses on the structure and functioning of the pelagic$\backslash$nfoodweb after upwelling predicts rapid growth of a phytoplankton$\backslash$ncommunity dominated by netphytoplankton and chain-forming nanophytoplankton$\backslash$ncells. After nitrate-depletion the bloom declines, to be followed$\backslash$nby a bloom of single nanophytoplankton cells dependent upon regenerated$\backslash$nnitrogen. During conditions of high nitrate availability and a netphytoplankton-dominated$\backslash$ncommunity, mesozooplankton ingest 44 per cent of the total primary$\backslash$nproduction through herbivory and 1.4 per cent through carnivory.$\backslash$nDuring periods of low nitrate availability and a nanophytoplankton-dominated$\backslash$ncommunity, mesozooplankton ingest 0.6 per cent of the total primary$\backslash$nproduction through hervivory and 5 per cent through carnivory. Food$\backslash$nchains are longer and microheterotrophs are an important link between$\backslash$nprimary producers and the larger heterotrophs. Simulation results$\backslash$nshow that microheterotrophs are an important component of the pelagic$\backslash$nfoodweb, primarily as regenerators of nitrogen, which sustains phytoplankton$\backslash$ngrowth, and as a food source for larger heterotrophs of the metazoan$\backslash$nfoodweb during the nanophytoplankton-dominated bloom},
author = {Painting, S. J. and Moloney, C. L. and Probyn, T. A. and Tibbles, B.},
doi = {10.2989/02577619209504723},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Microheterotrophic pathways in the southern Benguela upwelling system}},
year = {1992}
}
@article{VanWaerebeek1995,
abstract = {Prodelphinus petersii Lutken, 1889 from Amsterdam Island is assigned as a junior synonym of the dusky dolphin Lagenorhynchus obscurus (Gray, 1828). This identification and three confirmed sightings from near the Prince Edward Islands represent the first records of the dusky dolphin from the Indian Ocean. L. obscurus is documented also for the first time from the mid-Atlantic near Gough Island, and its much disputed occurrence off the Falklands Islands is confirmed. Possible sightings of L. obscurus from both the southern Indian and Atlantic oceans are reviewed. Available evidence supports a disjunct distribution for L. obscurus and the existence of discrete stocks confined to continental shelves and oceanic island groups. However, the taxonomic status of Lagenorhynchus species sighted in offshore tropical waters needs to be clarified.},
author = {{Van Waerebeek}, K. and {Van Bree}, P. J. and Best, P. B.},
doi = {10.2989/025776195784156584},
isbn = {0257-7615},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{On the identity of Prodelphinus petersii l{\"{u}}tken, 1889 and records of dusky dolphin Lagenorhynchus obscurus (Gray, 1828) from the Southern Indian and Atlantic Oceans}},
year = {1995}
}
@article{Shannon2004c,
abstract = {A workshop was held in Cape Town in December 2002 to introduce the concept of an ecosystem approach to fisheries (EAF) management in the southern Benguela, and to examine the options for implementing an EAF in South Africa. The workshop considered alternative modelling approaches that may have potential for an ecosystem approach to fisheries. Consensus was that an EAF should be implemented in South Africa through an incremental process, starting immediately. Ecosystem models can be used to provide guidance on reference points and broader management objectives still currently set on the basis of single-species assessments. Such additional information would be incorporated into the decision-making process, and comments received at a management level would also feed back to the modelling process. It was suggested that, at the scientific level, an ecosystem modelling perspective could be incorporated into existing single-species management recommendations by testing them with ecosystem models. Compilation of an “ecosystem considerations” document was recommended to initiate the process. It was proposed that a dedicated EAF working group be established in South Africa to advise on the process of implementing an EAF in the various fisheries, and to provide overarching guidance and to ensure consistency in integrating existing data and information for informing the management process.},
author = {Shannon, L J and Cochrane, K L and Moloney, C L and Freon, P},
doi = {10.2989/18142320409504046},
journal = {African Journal of Marine Science},
keywords = {Ecosystem approach to fisheries,ecosystem modelling,fisheries management,southern Benguela},
title = {{Ecosystem Approach to Fisheries Management in the Southern Benguela: A Workshop Overview}},
year = {2004}
}
@article{Cedras2011,
abstract = {The bearded goby Sufflogobius bibarbatus has come to replace sardine Sardinops sagax in the diets of many top predators within the marine environment off Namibia, and it is playing a key role within the region's foodweb. Previously published information on the diet of the bearded goby has been derived from small samples and has been contradictory. We investigated the gut contents of over 3 500 specimens of S. bibarbatus from two separate studies. The results indicate that the species has a fairly generalised diet, feeding mostly on benthic and pelagic animals (phytoplankton was rare), in a way that reflects ontogeny, habitat and likely prey availability. Smaller, more pelagic individuals included a substantial number of pelagic organisms in their diet and the proportion of benthic organisms increased with the size of the goby. Given the importance of this species in recycling benthic carbon through the foodweb of the northern Benguela ecosystem, there is a need to describe and quantify the benthic food environment.},
author = {Cedras, R. B. and Salvanes, A. G.V. and Gibbons, M. J.},
doi = {10.2989/1814232X.2011.600431},
isbn = {1814-232X},
issn = {1814232X},
journal = {African Journal of Marine Science},
keywords = {Benguela,Benthic-pelagic coupling,Ecosystem approach to fisheries,Infauna,Polychaetes,Zooplankton},
title = {{Investigations into the diet and feeding ecology of the bearded goby Sufflogobius bibarbatus off Namibia}},
year = {2011}
}
