@article{Jury1991,
abstract = {Aircraft observations were used to investigate contrasting weather$\backslash$nconditions over the northern edge of the warm Agulhas Current south of$\backslash$nGeorge, South Africa, during June 1989. A sea surface temperature front$\backslash$nof 7-degrees-C over 10 km was observed near 35-degrees-S, 23-degrees-E.$\backslash$nSurface-sensible and latent heat fluxes increased by a factor of five on$\backslash$nthe warm side. A deepening of the moist unstable layer was indicated by$\backslash$nincreased equivalent potential temperatures over the warm side. Most$\backslash$nsignificantly, convectively generated turbulence, as inferred from$\backslash$nvariances in air pressure, increased exponentially with sea surface$\backslash$ntemperature and surface layer winds doubled south of 35-degrees-S. The$\backslash$nsea state, initially smooth over the continental shelf, became rough$\backslash$nseawards of the northern edge of the Agulhas Current. Numerical model$\backslash$nsimulations were performed to assess air-sea interactions.$\backslash$nModel-simulated thermodynamic variables responded in close agreement$\backslash$nwith observations, but the increase in cross-frontal winds was not fully$\backslash$nresolved. The aircraft observations provide evidence that the Agulhas$\backslash$nCurrent affects the large-scale pressure field and may enhance the$\backslash$nrain-bearing capacity of transient weather systems.},
author = {Jury, M. R. and Courtney, S.},
doi = {10.2989/02577619109504629},
isbn = {0257761910950},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{A transition in weather over the Agulhas Current}},
year = {1991}
}
