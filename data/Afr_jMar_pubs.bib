title = {{Age and growth of South African silver kob {\textless}i{\textgreater}argyrosomus inodorus{\textless}/i{\textgreater} (Sciaenidae), with evidence for separate stocks}},
year = {1996}
}
@article{Zoutendyk1988a,
abstract = {To determine long-term consumption rates of the rock lobster Jasus lalandii, aquarium-held males in the size range 80-130 mm carapace length were fed mussels Choromytilus meridionalis over a period of 400 d. Annual patterns in consumptions were closely linked to breeding and moulting cycles. Feeding activity was fastest in summer, declined in autumn during breeding and was at its lowest in winter prior to moulting. There was a pre-ecdysis period of mean duration of 44 d when consumption was zero, followed by a post-ecydsis period of a further 34 d without feeding. The impact of J. lalandii in a sanctuary at Oudekraal is equivalent to removing 107 C. meridionalis (50 mm long).cntdot.m-2.cntdot.year (or 23,8 gC.cntdot.m-2.cntdot.year-1). Using rock-lobster size-frequency data from Robben Island sanctuary, the impact of the J. lalandii population there would be to remove 35,3gC.cntdot.m-2.cntdot.year-1 from prey populations.$\backslash$n},
author = {Zoutendyk, P.},
