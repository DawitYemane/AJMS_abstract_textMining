@article{M1989,
abstract = {Samples of juvenile anchovy Engraulis capensis were collected from commercial and research catches made off the west coast of South Africa to investigate variations in the growth rate by month and by locality of capture. Age of fish was determined by counting daily increments in otoliths viewed by means of a scanning electron microscope. Estimated growth rates were highly variable, resulting in birthdate distributions spanning six or seven months for similarly sized fish caught in the same month. Significant differences in growth rate were recorded between months, the slowest growth rates occurring in anchovy which had hatched during the period of peak spawning in late spring and early summer. Growth rates of juvenile anchovy of the 1986 recruiting year-class were approximately 25 per cent lower than those of previous year-classes sampled for daily growth analysis.},
author = {M, Waldron and M j, Armstrong and R m, Prosch},
issn = {0257-7615},
