@article{Steinke1993a,
abstract = {Feeding of Sesarma meinerti de Man was studied in the laboratory and at the Beachwood Mangrove Reserve. n the field, mangrove leaf litter formed 74,6{\%} of the diet. Crabs showed a preference for leaves in the order: yellow Bruguiera gymnorrhiza (L.) Lam. {\textgreater} yellow Avicennia marina (Forssk.) Vierh. {\textgreater} green B. gymnorrhiza {\textgreater} green A. marina. In the laboratory, a positive correlation was observed between relative consumption and degree of decomposition. Crabs fed with both tannin and tannin-free leaves showed a preference for the latter. Removal of B. gymnorrhiza leaves by S. meinerti was high throughout the year, whereas dry mass toss attributable to consumption varied according to season. It is suggested that S. meinerti plays an important role in the degradation of mangrove leaf litter.},
author = {Steinke, T D and Rajh, S and Holland, A J},
journal = {South African Journal of Marine Science},
keywords = {Beachwood Mangrove Reserve,Bruguiera,Leaf litter,Sesarma meinterti,avicennia,crabs,litter processing,mangrove},
title = {{The feeding behavious of the red mangrove crab Sesarma meinterti - de Man, 1887 (Crustacea, Decapoda, Grapsidae) and its effect on the degradaion of mangrove leaf-litter}},
