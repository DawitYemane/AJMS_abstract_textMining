title = {{Biology of the cirrate octopod {\textless}i{\textgreater}Grimpoteuthis glacialis{\textless}/i{\textgreater} (Cephalopoda; Opisthoteuthididae) in the South Shetland Islands, Antarctica}},
year = {1998}
}
@article{Ebert1991e,
abstract = {The diets of 15 species of squaloid sharks caught between Walvis Bay and Cape Agulhas over a depth range of 50-1016 m are analysed. The most common prey items were fish and cephalopods. Hake Merluccius spp. were common in the diets of Centrophorus squamosus, Squalus acanthias , and S. cf. mitsukurii . Myctophids were extensively preyed on by Centroscyllium fabricii, Deania calcea, D. profundorum, Etmopterus cf. brachyurus, E. compagnoi, S. acanthias and S. megalops , among others. The most common cephalopods in shark stomachs were Histioteuthis miranda, Lycoteuthis? diadema, Octopus vulgaris and Todarodes angolensis . Crustaceans were uncommon prey of most species, except for Centroscyllum fabricii and Etmopterius cf. granulosus . Centroscymnus coelolepis was the only squaloid with cetacean remains in the stomach. That species appears to parasitize its cetacean victims.},
author = {Ebert, D. a. and Cowley, P. D. and Compagno, L. J. V.},
doi = {10.2989/02577619209504727},
isbn = {0621111694},
issn = {0257-7615},
journal = {South African Journal of Marine Science},
pmid = {948},
title = {{A preliminary investigation of the feeding ecology of squaloid sharks off the west coast of southern Africa}},
year = {1991}
}
@article{Abello1990,
abstract = {The distribution patterns of the benthic stomatopod crustacean Pterygosquilla armata capensis were studied off southern Namibia between latitudes 23 and 30 degree S and between about 50 and 500 m depth. Whereas south of 28 degree S and off the South African coast this species is mainly a coastal animal distributed on soft substrata at depths shallower than 150-200 m, the maximum densities in Namibia occur in much deeper waters (200-350 m). In winter, maximum relative densities of the species are found shallower ({\textless} 200 m depth) than in summer (200-300 m). This density shift is thought to be related to the fact that, in summer, the main upwelling period has passed and inshore oxygen levels on the bottom decrease to highly hypoxic values. The species avoids areas shallower than 200 m north of 27 degree S, the area where oxygen depletion is strongest during all seasons. Oxygen levels appear to be critical to the inshore distribution of the species, whereas temperatures of {\textless} 10-11 degree C seem to limit its distribution towards deeper waters. Highest abundances occur on sediments with 10-40 per cent silt-clay content. Population size structure did not show any apparent peaks which could be related to age cohorts.},
author = {Abell{\'{o}}, P. and Macpherson, E.},
doi = {10.2989/025776190784378934},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Influence of environmental conditions on the distribution of Pterygosquilla armata capensis (Crustacea: Stomatopoda) off Namibia}},
year = {1990}
}
@article{Majodina1996,
abstract = {The structural evolution of winter low pressure systems over the Southern Ocean is studied using ECMWF (European Community Medium - Range Weather Forecasts) gridded weather data for the period 1986-1992. A limited sample size (n = 14) of mid-latitude cyclone cases meeting prescribed criteria, equally divided between two areas south-west (west) and south-east (east) of Africa, are averaged and contrasted. Comparisons of composite features for west and east cases distinguish the influence of differing sea temperature regimes. Mid-latitude cyclones in the east entrain air overlying the 'warm pool' of the Agulhas Current recirculation gyre, and exhibit increased baroclinic structure, stronger uplift, enhanced zonal winds and greater water vapor content. Analysis of structural differences may assist marine forecasts and highlights the role that SST plays in modulating the intensity of mid-latitude cyclones south of Africa.},
author = {Majodina, M. and Jury, M. R.},
doi = {10.2989/025776196784158518},
isbn = {0257-7615},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Composite winter cyclones south of Africa: Evolution during eastward transit over the Agulhas warm pool}},
year = {1996}
}
@article{Pillay1989a,
abstract = {Nitrification in a closed prawn-culture system was weU established and followed the classical oxidation sequence of ammonia via nitrite to nitrate. Ammonia levels were in the range 4,8-68,7 J.lg-at N .1-1 , nitrite 0,15{\textperiodcentered}{\textperiodcentered}891 J.lg-at N .1-1 and nitrate approached a maximum of9 898 J.lg-at N .1-1 after 22 weeks. Marine nitrifying bacteria were enumerated by the most probable number (MPN) technique. The maximum MPN estimate of either group of nitrifying bacteria in the filter was 1,73 X 10'{\textperiodcentered}cm-' while the maximum MPN estimate of their counterparts in the culture water was approximately 500 times lower. To establish mean incubation times for the accurate enumeration of nitrifying bacteria, incubations were carried out over a period of 130 days. Maximum estimates of MPN of ammonia-oxidizing bacteria were obtained after an incubation period of 20 days with a mean of 15days. Nitrite-oxidizing bacteria required a maximum of65 days with a mean incubation period of 30,3 days. Nitrifikasie in 'n geslote sisteem vir die kweek van garnale was goed gevestig en het die klassieke oksidasie-volgorde van ammoniak oor nitriet na nitraat gevolg. Vlakke van ammoniak was binne die bestek 4,8-68,7 J.lg-at N {\textperiodcentered}1-', nitriet 0, I 5-891 J.lg-at N .1-1 en nitraat het'n maksimum van 9 898 J.lg-at N {\textperiodcentered}1-' bereik na 22 weke. Die mariene nitrifiserende bakteriee is deur middel van die tegniek van die waarskynlikste getal (M PN) getel. Die maksimum beraamde MPN van beide groepe van nitrifiserende bakteriee in die filter was 1,73 X 107.cm-3 terwyl die maksimum beraamde M PN van hierdie bakteriee in die kultuurwater omtrent 500 keer laer was. Om die gemiddelde inkubasietyd vir die akkurate beraming van nitrifiserende bakteriee te bepaal, is inkubasie oor 'n tydperk van 130 dae deurgevoer. Maksimum MPN-beramings van ammoniak-oksiderende bakteriee is na inkubasie vir 20 dae behaal met 'n gemiddelde van 15 dae. Nitriet-oksiderende bakteriee het 'n maksimum van 65 dae benodig met 'n gemiddelde inkubasietyd van 30,3 dae. 325},
author = {Pillay, B. and Roth, G. and Oellermann, R. A.},
doi = {10.2989/02577618909504572},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Nitrification and the enumeration of marine nitrifying bacteria in a closed prawn-culture system}},
year = {1989}
}
@article{Groeneveld1995b,
abstract = {The breeding period in female P. gilchristi was determined by integrating periods of higher egg-bearing proportions with the preceding ripening of ovaries. Most ovaries in the central sector ripened in June (c. 75{\%} in Stage 4), followed by spawning in July/August. Most females (all size-classes) bear eggs between July (40,0{\%}) and November (50,2{\%}). A significant percentage of females with a carapace length {\textgreater}80 mm also bear eggs in March (50,5{\%}). Arguments for a single, bimodally distributed breeding cycle as opposed to two spawnings per year for large females are given. The relationship between breeding period and rock lobster size along the coast was examined for a number of years of data.},
author = {Groeneveld, J. C. and Rossouw, G. J.},
