@article{Gruss2016a,
abstract = {We developed delta generalised additive models (GAMs) to predict the$\backslash$nspatial distribution of different size classes of South African hakes,$\backslash$nMerluccius capensis and M. paradoxus, using demersal trawl survey data$\backslash$nand geographical (latitude and longitude) and environmental features$\backslash$n(depth, temperature, bottom dissolved oxygen and sediment type). Our$\backslash$napproach consists of fitting, for each hake size class, two independent$\backslash$nmodels, a binomial GAM and a quasi-Poisson GAM, whose predictions are$\backslash$nthen combined using the delta method. Delta GAMs were validated using an$\backslash$niterative cross-validation procedure, and their predictions were then$\backslash$nemployed to produce distribution maps for the southern Benguela. Delta$\backslash$nGAM predictions confirmed existing knowledge about the spatial$\backslash$ndistribution patterns of South African hakes, and brought new insights$\backslash$ninto the factors influencing the presence/absence and abundance of these$\backslash$nspecies. Our GAM approach can be used to produce distribution maps for$\backslash$nspatially explicit ecosystem models of the southern Benguela in a$\backslash$nrigorous and objective way. Ecosystem models are critical features of$\backslash$nthe ecosystem approach to fisheries, and distribution maps constructed$\backslash$nusing our GAM approach will enable a reliable allocation of species$\backslash$nbiomasses in spatially explicit ecosystem models, which will increase$\backslash$ntrust in the spatial overlaps and, therefore, the trophic interactions$\backslash$npredicted by these models.},
author = {Gruss, A and Yemane, D and Fairweather, T P},
doi = {10.2989/1814232X.2016.1218367},
issn = {1814-232X},
journal = {AFRICAN JOURNAL OF MARINE SCIENCE},
keywords = {distribution maps; Merluccius capensis; Merluccius},
title = {{Exploring the spatial distribution patterns of South African Cape hakes using generalised additive models}},
year = {2016}
}
