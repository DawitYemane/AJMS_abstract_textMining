@article{Bianchi2001,
abstract = {Demersal fish surveys carried out off Namibia from 1990 to 2000 were used to study the dynamics of demersal fish assemblages on the shelf and upper slope, The study was performed on each major assemblage, i.e. over the shelf (100-300 m deep) and the slope (300-600 m). Changes in size spectrum, diversity, species composition and trends in abundance of the dominating species were analysed. The most evident changes were those related to overall abundance and diversity of the slope assemblage.},
author = {Bianchi, G. and Hamukuaya, H. and Alvheim, O.},
