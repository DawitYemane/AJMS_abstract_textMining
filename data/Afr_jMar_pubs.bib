@article{Ebert1991c,
abstract = {The sevengill shark commonly occurs in most temperate seas and is a distinctly coastal species, rarely venturing below a depth of 100 m. It feeds primarily on sharks, batoids, teleosts, pinnipeds and cetaceans, but crustaceans, cephalopoda, algae, gastropoda and agnatha have also been reported. However, algae and gastropoda are miscellaneous prey items since those were found in association with egg cases.},
