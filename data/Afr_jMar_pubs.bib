@article{Wright2016,
abstract = {Marthasterias glacialis are found in the cool-temperate waters of the$\backslash$nnorth-eastern Atlantic Ocean, in the subtropical waters of the$\backslash$nMediterranean Sea and along the south-western tip of Africa. The South$\backslash$nAfrican Marthasterias population includes two morphotypes, a smooth,$\backslash$nspineless rarispina form and a spiny africana form, that have been$\backslash$ndescribed as separate species, subspecies, or forma by various authors$\backslash$nover the past century. To test whether these two morphotypes represent$\backslash$nseparate species, and whether either, or both, are conspecific with the$\backslash$nNorth-East Atlantic species, 78 Marthasterias were collected from the$\backslash$nCape Peninsula of South Africa. Morphological comparisons between$\backslash$nindividuals of the two forms showed no significant clustering of$\backslash$nsamples, indicating that there is no morphological separation of the two$\backslash$nSouth African forms into distinct groupings. The africana and rarispina$\backslash$nforms were also shown to be genetically indistinguishable, using a$\backslash$nfragment of the mitochondrial cytochrome c oxidase subunit I (COI) gene$\backslash$nand the nuclear internal transcribed spacer 1 (ITS1). In addition, the$\backslash$nCOI sequences were also compared to those from European specimens, and$\backslash$nphylogenetic reconstruction and intra-and interspecific levels of$\backslash$ndivergence suggested that the South African specimens form a single$\backslash$ngroup that is genetically distinct from the European M. glacialis.$\backslash$nAlthough the allopatric distribution, high genetic divergence (more than$\backslash$n3{\%} for the COI fragment) and morphological differences suggest that the$\backslash$nSouth African form should be raised to species status under the name$\backslash$nMarthasterias africana, further work must assess an independent genetic$\backslash$nmarker (nuclear) to support raising the COI clade to species level. True$\backslash$nM. glacialis have a spine-armament pattern of a series of three or more$\backslash$nregular spine rows down the length of each arm, whereas M. africana are$\backslash$neither covered in many irregularly spaced spines, or have an$\backslash$nextraordinarily bare surface with only two spine rows per arm.$\backslash$nMarthasterias africana may also have an actinal spine simulating the$\backslash$npresence of a third inferomarginal spine. This work tentatively resolves$\backslash$nthe taxonomic dispute, elucidates the separation and amalgamation of the$\backslash$ntwo African forms and suggests a single, uniquely South African$\backslash$nMarthasterias species that might be distinct from the north Atlantic M.$\backslash$nglacialis, although further analyses to test reproductive isolation$\backslash$nbetween the North-East Atlantic and South African forms are required.},
author = {Wright, A. G. and P{\'{e}}rez-Portela, R. and Griffiths, C. L.},
doi = {10.2989/1814232X.2016.1222308},
issn = {18142338},
journal = {African Journal of Marine Science},
keywords = {genetics,morphology,taxonomy},
title = {{Determining the correct identity of South African Marthasterias (Echinodermata: Asteroidea)}},
year = {2016}
}
