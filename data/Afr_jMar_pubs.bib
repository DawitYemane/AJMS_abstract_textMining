@article{Miller1991,
abstract = {Four major phases in the exploitation of Antarctic marine living resources are identified and briefly described, viz. exploitation of seals, whales, finfish and krill Euphausia superba Dana. The pattern of exploitation has shifted from one group of resources to another. The recent efforts of the Commission for the Conservation of Antarctic Marine Living Resources are considered in relation to management of finfish exploitation and to the development of a scientifically based management plan for krill. Special emphasis is given to the data required for the effective management of the krill fishery, per se, given the current limited knowledge of the potential yield of the resource and in the face of uncertainty concerning key aspects of the species' biology/ecology. A recent South African initiative aimed at developing a practical management strategy for the Antarctic krill resource is described. Vier hooffases in die ontginning van Antarktiese lewende mariene hulpbronne word uitgeken en kortliks beskryf, te wete ontginning van robbe, walvisse, vinvisse en kril Euphausia superba Dana. Die ontginnings-patroon van die hulpbronne het van groep tot groep verskuif. Die onlangse pogings van die Commission for the Conservation of Antarctic Marine Living Resources word in oenskou geneem wat betref die bestuur van die ont-ginning van vinvisse en die ontwikkeling van 'n wetenskaplik gefundeerde bestuursplan vir kril. Besondere klem word gele op die gegewens wat benodig word vir die doeltreffende bestuur van die krilvissery, per se, gegee die huidige beperkte kennis oor die potensiele opbrengs van die hulpbron en die onsekerheid 001' sleutelaspekte van die spesies se biologie/ekologie. 'n Onlangse Suid-Afrikaanse inisiatief met die doel om 'n praktiese bestuurstrategie vir die Antarktiese krilhulpbron te ontwikkel, word beskryf.},
author = {Miller, D. G.},
doi = {10.2989/02577619109504642},
isbn = {0257-7615},
issn = {02577615},
journal = {South African Journal of Marine Science},
title = {{Exploitation of Antarctic marine living resources: A brief history and a possible approach to managing the krill fishery}},
year = {1991}
}
